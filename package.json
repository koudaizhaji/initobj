{
  "name": "init-obj",
  "private": true,
  "scripts": {
    "clean:type": "rimraf ./dist",
    "type:node": "tsc -p tsconfig.node.json --noEmit --composite false",
    "type:src": "pnpm run clean:type && vue-tsc -p tsconfig.src.json --composite false --declaration --emitDeclarationOnly",
    "mv-type": "tsx ./scripts/dts-mv.ts",
    "build:ui": "pnpm run type:src && pnpm --filter ./project/** run build && pnpm run mv-type",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx,.vue --fix ./",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:style": "stylelint --fix ./**/*.{css,scss,vue,html}",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "api:dev": "pnpm --filter ./project/base-api-obj run dev",
    "api:build": "pnpm --filter ./project/base-api-obj run build",
    "pc:dev": "pnpm --filter ./project/pc-vue3-ts run dev",
    "pc:build": "pnpm --filter ./project/pc-vue3-ts run build-only",
    "h5:dev": "pnpm --filter ./project/uniapp-mobile run dev:h5",
    "h5:build": "pnpm --filter ./project/uniapp-mobile run build:h5",
    "docs:dev": "pnpm --filter ./docs run dev",
    "docs:build": "pnpm --filter ./docs run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-define-config": "^1.24.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-stylistic": "^0.4.3",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.5"
  },
  "dependencies": {
    "md-editor-v3": "^4.10.0",
    "vue": "^3.3.4"
  }
}
